//Refrescamento do animador de 2 em 2 minutos
 $(document).ready(function() {
    var refreshId = setInterval(function() {
        $.ajax({
                type: "GET",
                url: "ajax/noar/noar.aspx?randval="+ Math.random(),
                success: function(html){
                $("#locutor").hide()
                    $("#locutor").fadeIn('normal',function(){
                        $("#locutor").html(html);
                    });
                }
             });
   }, 120000);
});
</script>


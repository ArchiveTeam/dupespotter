<script type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            type: "POST",
            cache: false,
            url: "ajax/cookies/avisoCookies.aspx",
            success: function (html) {
                if (html == "mostra") {
                    $("#avisoCookies").show();
                }
            }
        });
        $("#btnClose").click(function () {
            $("#avisoCookies").hide();
            $.ajax({
                type: "POST",
                cache: false,
                url: "ajax/cookies/avisoCookies.aspx?close=1",
                success: function (html) {}
            });
        });
    });
</script>

      if ($wamu_UserAgentRegex1.test(a) || $wamu_UserAgentRegex2.test(a.substr(0, 4))) {
        // Only redirect if necessary
        if (window.location.href != b) {
          window.location.replace(b);
        }
      }
    })(navigator.userAgent || navigator.vendor || window.opera, $wamu_MobileRedirectURL);    
    */

    // Redirect mobile users to mobile site
    function wamu_MobileRedirect(){
      var userAgent = navigator.userAgent;
      var agentChecker_obj = {
        iphone: userAgent.match(/(iPhone|iPod)/i),
        // blackberry: userAgent.match(/BlackBerry/i),
        android: userAgent.match(/Android.+mobile/i) // "mobile" specifies phones, however, some tablets still report mobile
      };
      
      if (agentChecker_obj.android || agentChecker_obj.iphone) {
        if (window.location.href != $wamu_MobileRedirectURL) {
          window.location.replace($wamu_MobileRedirectURL);
        }
      }
    }
    wamu_MobileRedirect();
    


  </script>


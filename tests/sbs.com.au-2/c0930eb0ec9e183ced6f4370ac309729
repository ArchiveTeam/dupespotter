(function($) {
  "use strict";
  if (Drupal.verticalTab.prototype.focus) {
    Drupal.verticalTab.prototype.focus = function () {
      if (Modernizr.csstransforms && this.fieldset.parents('.field-group-tabs-wrapper').hasClass('group-kit')) {
        var index = this.fieldset.parent().children('fieldset.vertical-tabs-pane').index(this.fieldset);
        this.fieldset.parent()
          .removeClass(function(i,c) {
            var classes = c.match(/focus-[0-9]+/);
            return classes ? classes.join(' ') : '';
          })
          .addClass('focus-' + index);
        this.fieldset
          .siblings('fieldset.vertical-tabs-pane')
            .each(function () {
              var tab = $(this).data('verticalTab');
              tab.item.removeClass('selected');
            })
            .end()
          .siblings(':hidden.vertical-tabs-active-tab')
            .val(this.fieldset.attr('id'));
      } else {
        this.fieldset
          .siblings('fieldset.vertical-tabs-pane')
            .each(function () {
              var tab = $(this).data('verticalTab');
              tab.fieldset.hide();
              tab.item.removeClass('selected');
            })
            .end()
          .show()
          .siblings(':hidden.vertical-tabs-active-tab')
            .val(this.fieldset.attr('id'));
      }
      this.item.addClass('selected');
      // Mark the active tab for screen readers.
      $('#active-vertical-tab').remove();
      this.link.append('<span id="active-vertical-tab" class="element-invisible">' + Drupal.t('(active tab)') + '</span>');
    };
  }
  if (Drupal.theme.prototype.verticalTab) {
    var func = Drupal.theme.prototype.verticalTab;
    Drupal.theme.prototype.verticalTab = function (settings) {
      var tab = func(settings);
      tab.item.addClass('vertical-tab-button-' + settings.title.toLowerCase());
      return tab;
    };
  }
})(jQuery);

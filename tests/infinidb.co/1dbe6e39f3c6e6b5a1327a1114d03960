<script>
    $( document ).ready(function() {
    var optionTexts = [];
    var regErrors = ["E-mail address field is required.", "First Name field is required.", "Last Name field is required.", "Company field is required.", "State field is required.", "Phone field is required.", "Country field is required."];
$(".error ul li").each(function() { optionTexts.push($(this).text()) });
for (var i = 0; i < regErrors.length; i++) {
    if($.inArray(regErrors[i], optionTexts) > -1){
        $("#register-form").show();
        $("#login-form").hide();
        $("a#login-link").removeClass("lt-active");
        $("a#register-link").addClass("lt-active");
        break;
}
}
});
</script>      </body>

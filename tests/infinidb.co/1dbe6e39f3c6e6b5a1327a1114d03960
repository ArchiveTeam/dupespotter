    <script>
        $(document).ready(function() {
            //jquery
            $(location).attr('href');

            //pure javascript
            var pathname = window.location.pathname;
            var pagename = pathname.split("/").pop();
            var finalURL = pathname.replace(pagename,'');
            var newpath = finalURL.replace(/\/$/, '');

            $('#nav li a[href$="' + pathname + '"]').parent().addClass('current');
            $('.data-wrap-inner li a[href$="' + pathname + '"]').parent().addClass('current');
            $('#left-sidebar-menu li a[href$="' + pathname + '"]').parent().addClass('current');
            $('.cnt-menu li a[href$="' + newpath + '"]').parent().addClass('current');

            if (pathname.toLowerCase().indexOf("about/") >= 0) {
                var about_name = "/about";
                $('.web-menu li a[href$="' + about_name + '"]').parent().addClass('current');
                $('#left-sidebar-menu li a[href$="' + newpath + '"]').parent().addClass('current');
            }

            if (pathname.toLowerCase().indexOf("/community/announcements") >= 0) {
                $(".forum-node-create-links").remove();
                $('body').addClass('forum-annoucements');
            }

            //forum pages
            if ($("body").hasClass("page-forum") || $("body").hasClass("node-type-forum")) {
              $('.data-wrap-inner li a[href$="'+newpath+'"]').parent().addClass('current');
              var forum_name = "/community";
              $('.web-menu li a[href$="' + forum_name + '"]').parent().addClass('current');
                $('.data-wrap-inner li a[href$="community"]').parent().addClass('current');
            }
            if ($('body').hasClass('page-node-add-forum')) {
                $('.data-wrap-inner li a[href$="community"]').parent().addClass('current');
            }

            //blog pages
           if ($("body").hasClass("page-blog") || $("body").hasClass("node-type-blog")) {
                $(".web-only li").last().addClass('current');
            }

            //career category pages
            if (pathname.toLowerCase().indexOf("careers/") >= 0) {
                $('.web-menu li a[href$="about"]').parent().addClass('current');
                $('#left-sidebar-menu li a[href$="/about/careers"]').parent().addClass('current');
            }

            //events page
            if ($("body").hasClass("page-resources-events")) {
              $('.data-wrap-inner li a[href$="'+newpath+'"]').parent().addClass('current');
              // $(".web-menu li").last().addClass('current');
            }

            //forum main list
            if (pathname == "/forum" || pathname == "/community"  || pathname == "/community/" || $('body').hasClass('page-forum-')) {
                $('body').addClass('forum-main');
            }
                    
            //products and downloads page
            if (pathname == "/products" || pathname == "/try-infinidb") { 
                $(".accordion .accord-header").click(function() {
                    if($(this).next("div").is(":visible")){
                        $(this).removeClass('activated');
                    $(this).next("div").slideUp("slow");
                    } else {
                        $('.activated').removeClass('activated');
                        $(this).addClass('activated');
                        $(this).next("h2").removeClass('activated');
                        $(".accordion .accord-content").slideUp("slow");

                    $(this).next("div").slideToggle("slow");
                    }
                });
            }

            //resources page
            if (pathname == "/resources") { 
                $(".accordion .accord-header").click(function() {
                    if($(this).next("div").is(":visible")){
                        $(this).removeClass('activated');
                        $(this).next("div").slideUp("slow");
                    } else {
                        $('.activated').removeClass('activated');
                        $(this).addClass('activated');
                        $(this).next("h2").removeClass('activated');
                        $(".accordion .accord-content").slideUp("slow");
                        $(".inner-accord-content").hide();
                        $(this).next("div").slideToggle("slow");

                    }
                });

                $(".inner-accordion .inner-accord-header").click(function() {
                    $(this).parent().parent().parent().find("h2.accord-header").addClass('activated');
                    if($(this).next("div").is(":visible")){
                        $(this).removeClass('activated');
                        $(this).next("div").slideUp("slow");
                    } else {
                        $('h2.inner-accord-header.activated').removeClass('activated');
                        $(this).addClass('activated');
                        $(this).next("h2.inner-accord-header").removeClass('activated');
                        $(".inner-accordion .inner-accord-content").slideUp("slow");
                        $(this).next("div").slideToggle("slow");
                    }
                });
            }

        });
    </script>

    <script type="text/javascript">
      $(document).ready(function() {
      //hide div on outside click
        $(document.body).not(".login .list-inner-cnt").click(function () {
          $('li.login div.list-inner-cnt').hide();
        });
      //login block error
        if ($('#user-login-block-container input[type=text]').hasClass('error') || $('#user-login-block-container input[type=password]').hasClass('error')) {
          $('div.messages').hide();
          $('li.login div.list-inner-cnt').show();
          $('.login-error').show();
          $('li.login').addClass('user-block-bg');
        }
    });
  </script> 
    

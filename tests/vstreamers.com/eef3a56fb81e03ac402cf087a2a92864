<script type="text/javascript">
	var isOverLogin = 0;
	$('#login_text').mouseover(function() {
		$('#header_login').show();
	});

	$(document).mousemove(function(e) {
		var mousey = e.pageY;
		var mousex = e.pageX;
		var obj = $('#header_login');
		if (obj.css('display') == 'block' && mousey <= (obj.offset().top + obj.outerHeight()) && mousex >= obj.offset().left && mousex <= (obj.offset().left + obj.outerWidth())) {
			isOverLogin = 1;
		} else {
			if (isOverLogin == 1) {
				isOverLogin = 0;
				$('#header_login').hide();
			}
		}
	});

	function submitLogin(t) {
		if (t == undefined) { t = 'login'; }
		
		if (t == 'login') {
			var uName = encodeURIComponent($('#login_username').val());
			var uPass = encodeURIComponent($('#login_password').val());
			var uRemember = 0;
			if ($('#login_remember').is(':checked')) { uRemember = 1; }
			var addParams = '&l_user='+uName+'&l_pass='+uPass+'&l_remember='+uRemember;
			var oldObj = $('#login_button');
		}
		
		if (t == 'forgotpass') {
			var addParams = '&l_email=' + encodeURIComponent($('#forgotpass_email').val());
			var oldObj = $('#forgotpass_button');
		}
		
		var oldObjTxt = oldObj.html();
		oldObj.addClass('bt_pressed').html('<img src="../images/loading.gif" height="23"></img>');
		
		$.ajax({
			url: '../js/login_form.php',
			type: 'post',
			dataType: 'json',
			data: 'action=' + t + addParams,
			success: function(r) {
				if (r[0] == 0) {
					$('.loginErrorDiv').html(r[1]);
				}
				
				if (r[0] == 1) {
					location.reload();
				}
				
				isOverLogin = 0;
				$('#header_login').hide();
				oldObj.removeClass('bt_pressed').html(oldObjTxt);
			}
		});
	}
	
	$('#login_username').keypress(function(e) { if (e.keyCode == 13) { submitLogin(); } });
	$('#login_password').keypress(function(e) { if (e.keyCode == 13) { submitLogin(); } });
	$('#login_button').click(function() { submitLogin(); });
	$('#forgotmypass_bt').click(function() { $('#forgotpass_terminal').removeClass('hidden'); $('#login_terminal').addClass('hidden'); });
	$('#forgotpass_email').keypress(function(e) { if (e.keyCode == 13) { submitLogin('forgotpass'); } });
	$('#forgotpass_button').click(function() { submitLogin('forgotpass'); });
	$('#forgotpass_cancel').click(function() { $('#forgotpass_terminal').addClass('hidden'); $('#login_terminal').removeClass('hidden'); });
	
	$('.loginErrorDiv').on('click', '#resend_act_email a', function() {
		var oldObj = $(this).parent();
		oldObj.html('<img src="../images/loading.gif" height="12">');

		$.ajax({
			url: '../js/resend_email.php',
			type: 'post',
			data: 'u_id=' + $('#resend_act_email').attr('data-resend-uid') + '&u_pass=' + $('#resend_act_email').attr('data-resend-pass'),
			success: function(r) {
				oldObj.html(r);
			}
		});
	});
</script>	</div>


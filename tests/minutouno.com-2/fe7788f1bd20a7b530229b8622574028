    <script>
      var srcClima = "00003";
      if(Cookie.read("M1Clima")){
        srcClima = Cookie.read("M1Clima");
        Cookie.write('M1Clima', srcClima, {duration: 30});
        
        for( var i = 0 ; i < $("climaSelect").length ; i++ ){
          if($("climaSelect").options[i].value == srcClima){
            $("climaSelect").selectedIndex = i;
          }
        }
      }
      
      document.write("<scr"+"ipt src='http://servicios.nuestroclima.com/clientes/00064/"+srcClima+"S.js'></sc"+"ript>");
      document.write("<scr"+"ipt src='http://servicios.nuestroclima.com/clientes/00064/"+srcClima+"H.js'></sc"+"ript>");
      document.write("<scr"+"ipt src='http://servicios.nuestroclima.com/clientes/00064/"+srcClima+"Q.js'></sc"+"ript>");
    </script>
  <script>
  window._horaServer = 0;
  if(typeof(window.enableClock) == "undefined"){
    var myRequest_time = new Request({
      url       : '/_post/minutouno/getTimeStamp.php',
      method    : 'get',
      onSuccess : function(r){
          window._horaServer = r;
      }
    });

    myRequest_time.send();

    window.showClock = (function(){
      window._horaServer++;
      var Digital   = new Date(window._horaServer * 1000);
      var hours     = Digital.getHours()
      var minutes   = Digital.getMinutes()
      var seconds   = Digital.getSeconds()

      if(minutes <= 9) minutes = "0"+minutes;
      if(seconds <= 9) seconds = "0"+seconds;
      document.getElementById('server_clock').innerHTML = hours + ":" + minutes + ":" + seconds + " ";
    });
    window.enableClock = setInterval(window.showClock,1000);
  }
</script>    



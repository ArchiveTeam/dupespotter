<script type="text/javascript" xmlns:deferer="no">
window.m1_climaRenderLayer_GUARDED = function(content){
    var BASE_PATH = $('contentClima').get('base');
    content	= content.replace(/images/gi, BASE_PATH + 'css/minutouno_images');
    $("contentClima").innerHTML = content;
    window.NCCall = function(){};	   
};

  function m1_climaRenderLayer(content) {	
        if(!window.m1_climaRenderLayer_GUARDED) {
                setTimeout(function() {
                        m1_climaRenderLayer(content);
                }, 100);
                return;
        }
        window.m1_climaRenderLayer_GUARDED(content);
};
  
  var changeClima = function(combo){
    //Recargamos el js
    srcSelected = combo.value;
    var head= document.getElementsByTagName('head')[0];
    var script= document.createElement('script');
    script.type= 'text/javascript';
    script.src= 'http://servicios.nuestroclima.com/clientes/00064/'+srcSelected+'S.js';
    head.appendChild(script);
    
    var script= document.createElement('script');
    script.type= 'text/javascript';
    script.src= 'http://servicios.nuestroclima.com/clientes/00064/'+srcSelected+'H.js';
    head.appendChild(script);

    var script= document.createElement('script');
    script.type= 'text/javascript';
    script.src= 'http://servicios.nuestroclima.com/clientes/00064/'+srcSelected+'Q.js';
    head.appendChild(script);

    //Seteamos la cookie del clima
    Cookie.write('M1Clima', srcSelected, {duration: 30});
  }
</script>  

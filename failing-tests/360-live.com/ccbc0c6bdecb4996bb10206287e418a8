<script type="text/javascript" src="includes/js/jquery.imagesloaded.min.js"></script>	
<script type="text/javascript" src="includes/js/jquery.rwdImageMaps.min.js"></script>
<script>
function ready() {
	$('#cycler').css('height', $('#cycler a:first img').height());  //image exact resize at start
	$('#pager').fadeIn();
	bindClick();
} 	
	
function cycleImages($next) {
	$('.pagerItem').unbind('click');
	
	var $active = $('#cycler a img.active');
	if(typeof $next === "undefined") {
	    var $next = ($('#cycler a img.active').closest('a').next().find('img').length > 0) ? $('#cycler .active').closest('a').next().find('img') : $('#cycler a:first img'); 
	    $('#cycler').css('height', $next.height());
	}

	$next.css('z-index', 32); 

	$('div[target*="'+$next.attr('name')+'"]').addClass('selected');
	$('div[target*="'+$next.attr('name')+'"]').removeClass('clickable');	
	$('div[target*="'+$active.attr('name')+'"]').removeClass('selected');
	$('div[target*="'+$active.attr('name')+'"]').addClass('clickable');	

	$('.img_copyright').html($next.attr('copyright'));

	$active.fadeOut(700, function () {
	    $active.css('z-index', 31).show().removeClass('active');
	    $next.css('z-index', 33).addClass('active');
	    bindClick();
	
	});
}

function bindClick() {
	$('.clickable').bind('click', function() {
		clearInterval(timer); 
		cycleImages($('img[name*="'+$(this).attr('target')+'"]')  );  //resize if all pics have different sizes. 
	});
}


var timer=0;
$(document).ready(function () {
	
	$('#cycler a:first img').addClass('active');
	timer=setInterval('cycleImages()', 8000);
	
	//$('#cycler').css('height', $('#cycler a img.active').height());

	var i=0;
	$("#cycler a img").each(function(){
		i++;
		$("#pager").append('<div class="pagerItem clickable" target="'+$(this).attr('name') +'"><div>');
	});
	$('div[target*="slide0"]').addClass('selected');
	$('div[target*="slide0"]').removeClass('clickable');
	
	
	$('img[usemap]').rwdImageMaps();
	$("#cycler").imagesLoaded(ready);
});




</script>

